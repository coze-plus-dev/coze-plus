{
  "extends": "@coze-arch/ts-config/tsconfig.web.json",
  "include": ["src", "stories", "src/typings.d.ts"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ES2022",
    "lib": ["DOM", "ES2022", "ESNext"],
    "moduleResolution": "bundler",
    "target": "ES2020",
    "types": ["vitest/globals"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
